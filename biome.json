{
  "formatter": {
    "indentStyle": "space",
    "lineWidth": 140
  },
  "javascript": {
    "formatter": {
      "jsxQuoteStyle": "single",
      "quoteStyle": "single",
      "trailingCommas": "none",
      "indentStyle": "space",
      "semicolons": "asNeeded"
    }
  },
  "assist": {
    "enabled": true,
    "actions": {
      "source": {
        "organizeImports": {
          "level": "off"
        }
      }
    }
  }
}
